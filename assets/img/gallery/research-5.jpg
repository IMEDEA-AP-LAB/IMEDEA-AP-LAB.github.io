<svg width="400" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="vizGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e1b4b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f172a;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="200" fill="url(#vizGradient)"/>
  
  <!-- 3D visualization grid -->
  <g stroke="#475569" stroke-width="0.5" fill="none" opacity="0.6">
    <!-- Horizontal lines -->
    <line x1="50" y1="50" x2="350" y2="50"/>
    <line x1="60" y1="70" x2="340" y2="70"/>
    <line x1="70" y1="90" x2="330" y2="90"/>
    <line x1="80" y1="110" x2="320" y2="110"/>
    <line x1="90" y1="130" x2="310" y2="130"/>
    <line x1="100" y1="150" x2="300" y2="150"/>
    
    <!-- Vertical lines -->
    <line x1="50" y1="50" x2="100" y2="150"/>
    <line x1="100" y1="50" x2="130" y2="150"/>
    <line x1="150" y1="50" x2="160" y2="150"/>
    <line x1="200" y1="50" x2="190" y2="150"/>
    <line x1="250" y1="50" x2="220" y2="150"/>
    <line x1="300" y1="50" x2="250" y2="150"/>
    <line x1="350" y1="50" x2="300" y2="150"/>
  </g>
  
  <!-- Ocean circulation vectors -->
  <g stroke="#06b6d4" stroke-width="2" fill="#06b6d4">
    <!-- Large circulation pattern -->
    <path d="M 100 80 Q 200 60 300 80 Q 320 120 280 140 Q 200 160 120 140 Q 80 120 100 80" fill="none" opacity="0.7"/>
    <!-- Arrow heads -->
    <polygon points="300,80 295,75 295,85" opacity="0.8"/>
    <polygon points="280,140 285,135 285,145" opacity="0.8"/>
    <polygon points="120,140 125,135 125,145" opacity="0.8"/>
    <polygon points="100,80 105,75 105,85" opacity="0.8"/>
  </g>
  
  <!-- Temperature/velocity visualization -->
  <g>
    <rect x="120" y="90" width="20" height="20" fill="#ef4444" opacity="0.8"/>
    <rect x="140" y="90" width="20" height="20" fill="#f97316" opacity="0.8"/>
    <rect x="160" y="90" width="20" height="20" fill="#eab308" opacity="0.8"/>
    <rect x="180" y="90" width="20" height="20" fill="#22c55e" opacity="0.8"/>
    <rect x="200" y="90" width="20" height="20" fill="#06b6d4" opacity="0.8"/>
    <rect x="220" y="90" width="20" height="20" fill="#3b82f6" opacity="0.8"/>
    <rect x="240" y="90" width="20" height="20" fill="#8b5cf6" opacity="0.8"/>
  </g>
  
  <!-- Color scale -->
  <g fill="white" font-family="Arial, sans-serif" font-size="8" opacity="0.8">
    <text x="370" y="70">High</text>
    <text x="370" y="140">Low</text>
    <rect x="360" y="75" width="5" height="60" fill="url(#tempGradient)"/>
  </g>
  
  <defs>
    <linearGradient id="tempGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#ef4444;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#eab308;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <text x="200" y="190" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="14" font-weight="bold">
    Data Visualization
  </text>
</svg>
